<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Name</th>
            <th scope="col">Address</th>
            <th scope="col">Number</th>
        </tr>
    </thead>
    <tbody>
        <% data.forEach((person,index)=>{ %>
            <tr>
                <th scope="row">
                    <%= index+1 %>
                </th>
                <td>
                    <%=person.name %>
                </td>
                <td>
                    <%= person.address %>
                </td>
                <td>
                    <%= person.number %>
                </td>
                
            </tr>
          <%  }) %>
    </tbody>
</table>